<!DOCTYPE html>
<html lang="en" class="[scrollbar-gutter:stable]">
  <head>
    <div class="flex flex-row gap-4 p-4 bg-orange-100">
      <button
        phx-click={JS.navigate(~p"/users")}
        class="text-red-900 font-semibold hover:text-red-700"
      >
        JSusesrs
      </button>
      <.link navigate={~p"/users"} class="text-zinc-900 font-semibold hover:text-zinc-700">
        users
      </.link>
      <button
        phx-click={JS.navigate(~p"/stuffies")}
        class="text-red-900 font-semibold hover:text-red-700"
      >
        JSstuffies
      </button>
      <.link navigate={~p"/stuffies"} class="text-zinc-900 font-semibold hover:text-zinc-700">
        stuffies
      </.link>
    </div>

    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <meta name="csrf-token" content={get_csrf_token()} />
    <.live_title default="Test20" suffix=" Â· Phoenix Framework">
      {assigns[:page_title]}
    </.live_title>
    <link phx-track-static rel="stylesheet" href={~p"/assets/app.css"} />
    <script defer phx-track-static type="text/javascript" src={~p"/assets/app.js"}>
    </script>
  </head>
  <body class="bg-white">
    {@inner_content}
  </body>
</html>
